policy-options {
    policy-statement AWS-VPC-DX {
        term 1 {
            from as-path AWS-DX;
            then accept;
        }
    }
    policy-statement DEFAULT-ORIGINATE {
        term 1 {
            from {
                route-filter 0.0.0.0/0 exact;
            }
            then accept;
        }
    }
    policy-statement DN42-EXPORT {
        term 1 {
            from {
                route-filter 172.22.0.0/15 orlonger;
            }
            then accept;
        }
        term 2 {
            then reject;
        }
    }
