    policy-statement IPV4-RESTRICT-NH-RECURSION {
        term ACCEPT-DIRECTLY-CONNECTED-ROUTES {
            from protocol direct;
            then accept;
        }
        term ACCEPT-IPV4-SLASH-30-OR-LONGER-ROUTES {
            from {
                route-filter 0.0.0.0/0 prefix-length-range /30-/32;
            }
            then accept;
        }
        term REJECT-ALL {
            then reject;
        }
    }
    policy-statement IPV6-RESTRICT-NH-RECURSION {
        term ACCEPT-DIRECTLY-CONNECTED-ROUTES {
            from protocol direct;
            then accept;
        }
        term ACCEPT-IPV6-SLASH-126-OR-LONGER-ROUTES {
            from {
                route-filter 0::/0 prefix-length-range /126-/128;
            }
            then accept;
        }
        term REJECT-ALL {
            then reject;
        }
    }
    policy-statement REJECT-ALL {
        term 1 {
            then reject;
        }
    }
